   "outputs": [],
   "source": [
    "def district(x):\n",
    "    if x == \"Causeway Bay\" or x == \"Central\":\n",
    "        return \"HK\"\n",
    "    if x == \"Yuen Long\" or x == \"Tsuen Wan\":\n",
    "        return \"NT\"\n",
    "    else:\n",
    "        return \"KL\"\n",
    "    \n",
    "df['location'] = df['location'].apply(lambda x: district(x))"
   ]
  },
  {
