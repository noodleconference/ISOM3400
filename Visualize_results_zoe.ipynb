   "id": "60a39d06",
   "metadata": {},
   "outputs": [],
   "source": [
    "#change columns to float type\n",
    "df['happy'] = df['happy'].astype(float)\n",
    "df['sad'] = df['sad'].astype(float)\n",
    "df['bookmark'] = df['bookmark'].astype(float)"
   ]
