   "id": "2ae3f7a2",
   "metadata": {},
   "outputs": [],
   "source": [
    "#enable sort by price\n",
    "df['price'] = pd.Categorical(df['price'], ['Below $50','$51-100', '$101-200', '$201-400', '$401-800', 'Above $801'])"
   ]
